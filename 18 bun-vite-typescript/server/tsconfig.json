{
  "compilerOptions": {
    "types": ["bun-types"],
    "lib": ["ES5"],
    "module": "ES6",
    "target": "ES5",
    "moduleResolution": "bundler",
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "moduleDetection": "force",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "outDir": "dist"
  }
}
